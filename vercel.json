{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/seed.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/seed",
      "dest": "/api/seed.js"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
